// Read more about both here:
// https://www.prisma.io/docs/orm/prisma-migrate

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model UserExample {
  id                String              @id
  email             String
  name              String?
}

enum BlogImportType {
  LIVEJOURNAL
}

model BlogImport {
  id                String              @id
  rawJson           String
  type              BlogImportType
}
